<?php

function display_welcome_message($account_type) {
    if ($account_type === 'premium') {
        echo "Welcome Premium User!";
    } elseif ($account_type === 'basic') {
        echo "Welcome Basic User!";
    } else {
        echo "Unknown account type";
    }
}

// Example usage:
$user_account_type = 'premium';
echo "User Account Type: $user_account_type\n";
display_welcome_message($user_account_type);

?>
