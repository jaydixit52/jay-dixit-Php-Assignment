<?php

function assign_grade($score) {
    if ($score >= 90) {
        return 'Grade A';
    } elseif ($score >= 80) {
        return 'Grade B';
    } elseif ($score >= 70) {
        return 'Grade C';
    } elseif ($score >= 60) {
        return 'Grade D';
    } else {
        return 'Grade F';
    }
}

// Example usage:
$exam_score = 85;
$grade = assign_grade($exam_score);

echo "Exam score: $exam_score\n";
echo "Grade: $grade\n";

?>
